extends layout

block content
  .general-container
    form(action='/cifradocesar', method='GET' class='search-bar')
    


      select(name='cifrado')
        option(value='cifradocesar') Cifrado César
        option(value='base64') Base64
        option(value='hexadecimal') Hexadecimal
        option(value='binario') Binario
      button(type='submit')
        i(class='material-symbols-rounded') check  


          
     


    .centered-index-content
      h1 #{title}
      pp2     Bienvenido a la pagina de cifrado #1💯😍
